{
  "presets": [
    "@babel/preset-react",        
    [
      "@babel/preset-env", 
        {
          "modules": false
        }
    ]
  ],
  
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],

  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
          "@babel/preset-env" 
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    }
  }
}